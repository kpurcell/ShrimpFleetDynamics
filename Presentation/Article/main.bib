@ARTICLE{bacheler_spatial_2010,
  author = {Bacheler, Nathan M and Ciannelli, Lorenzo and Bailey, Kevin M and
	Duffy‐anderson, Janet T and Bacheler, Nathan M and Ciannelli, Lorenzo
	and Bailey, Kevin M and Duffy‐anderson, Janet T},
  title = {Spatial and temporal patterns of walleye pollock (\textit{Theragra
	chalcogramma}) spawning in the eastern Bering Sea inferred from egg
	and larval distributions, Spatial and temporal patterns of walleye
	pollock (Theragra chalcogramma) spawning in the eastern Bering Sea
	inferred from egg and larval distributions},
  journal = {Fisheries Oceanography, Fisheries Oceanography},
  year = {2010},
  volume = {19, 19},
  pages = {107, 107--120, 120},
  number = {2, 2},
  month = mar,
  abstract = {Walleye pollock Theragra chalcogramma (pollock hereafter) is a key
	ecological and economic species in the eastern Bering Sea, yet detailed
	synthesis of the spatial and temporal patterns of pollock ichthyoplankton
	in this important region is lacking. This knowledge gap is particularly
	severe considering that egg and larval distribution are essential
	to reconstructing spawning locations and early life stages drift
	pathways. We used 19 yr of ichthyoplankton collections to determine
	the spatial and temporal patterns of egg and larval distribution.
	Generalized additive models ({GAMs)} identified two primary temporal
	pulses of pollock eggs, the first occurring from 20 February to 31
	March and the second from 20 April to 20 May; larvae showed similar,
	but slightly lagged, pulses. Based on generalized cross-validation
	and information theory, a {GAM} model that allowed for different
	seasonal patterns in egg density within three unique areas outperformed
	a {GAM} that assumed a single fixed seasonal pattern across the entire
	eastern Bering Sea. This ‘area-dependent’ {GAM} predicted the highest
	densities of eggs (i.e., potential spawning locations) in three major
	areas of the eastern Bering Sea: near Bogoslof Island ({February–April)},
	north of Unimak Island and the Alaska Peninsula ({March–April)},
	and around the Pribilof Islands ({April–August).} Unique temporal
	patterns of egg density were observed for each area, suggesting that
	pollock spawning may be more spatially and temporally complex than
	previously assumed. Moreover, this work provides a valuable baseline
	of pollock spawning to which future changes, such as those resulting
	from climate variability, may be compared.},
  copyright = {© 2010 Blackwell Publishing Ltd},
  doi = {10.1111/j.1365-2419.2009.00531.x, 10.1111/j.1365-2419.2009.00531.x},
  file = {Bacheler et al. - 2010 - Spatial and temporal patterns of walleye pollock (.html:C:\Users\Kevin.Purcell\AppData\Roaming\Mozilla\Firefox\Profiles\a40irilq.default\zotero\storage\RQTE6E9A\Bacheler et al. - 2010 - Spatial and temporal patterns of walleye pollock (.html:text/html;Bacheler et al. - 2010 - Spatial and temporal patterns of walleye pollock (.pdf:C:\Users\Kevin.Purcell\AppData\Roaming\Mozilla\Firefox\Profiles\a40irilq.default\zotero\storage\XDZIZASD\Bacheler et al. - 2010 - Spatial and temporal patterns of walleye pollock (.pdf:application/pdf},
  issn = {1365-2419, 1365-2419},
  keywords = {drift, spawning, stock structure, Theragra chalcogramma, walleye pollock},
  language = {en, en},
  url = {http://onlinelibrary.wiley.com.proxy.lib.duke.edu/doi/10.1111/j.1365-2419.2009.00531.x/full},
  urldate = {2012-04-16}
}

@ARTICLE{bacheler_controls_2011,
